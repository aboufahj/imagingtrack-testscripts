            <system value="url"/>
            <value value="http://www.hl7.org/Special/committees/imagemgt/index.cfm"/>
        </telecom>
    </contact>
    <contact>
        <name value="Abderrazek Boufahja"/>
        <telecom>
            <system value="email"/>
            <value value="abderrazek.boufahja@ge.com"/>
        </telecom>
    </contact>
